{
    "name": "server",
    "type": "module",
    "main": "src/main.ts",
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch --env-file=.env src/main.ts",
        "start": "node --env-file=.env dist/main.js",
        "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\"",
        "docker:up": "docker compose up -d",
        "docker:down": "docker compose down"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "express-rate-limit": "^8.1.0",
        "helmet": "^8.1.0",
        "mongoose": "^8.18.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/node": "^24.4.0",
        "prettier": "^3.6.2",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    }
}
